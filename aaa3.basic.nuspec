<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<metadata>
		<id>aaa3.basic</id>
		<version>$version$</version>
		<authors>ParkNow</authors>
		<description>aaa3.basic Conatinerized Deployment Package</description>
		<language>en-US</language>
	</metadata>
	<files>
		<file src="templates\template*.yaml" target="templates" />
		<file src="metadata.yaml" target="metadata.yaml" />
		<file src="build\**" target="build" exclude="obj\**\*.*"/>
		<file src="build.ps1" target="build.ps1" />
		<file src="deploy\DeploymentScript.ps1" target="DeploymentScript.ps1" />
		<file src="src\aaa3.basic.WebApi\configs\**"   target="artifacts\configs"/>
		<file src="publish\integrations\**"   target="publish\integrations"/>
	</files>
</package>